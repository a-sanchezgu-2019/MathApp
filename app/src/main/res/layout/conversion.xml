<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/parentesis_dere">

    <ImageView
        android:id="@+id/parentesisiz"
        android:layout_width="429dp"
        android:layout_height="789dp"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:background="@drawable/fondo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/boton_config_calc_trig"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="332dp"
        android:layout_marginLeft="332dp"
        android:background="@drawable/ajustes_mini"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/cierraApp_calTrigo"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginBottom="4dp"

        android:adjustViewBounds="true"
        android:background="#0000"
        android:contentDescription="imagen menu"
        android:padding="0dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.725"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.021"
        app:srcCompat="@drawable/x" />
    <ImageButton
        android:id="@+id/boton_decalculadoraTrigo_amenu"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="16dp"

        android:adjustViewBounds="true"
        android:background="#0000"
        android:contentDescription="imagen menu"
        android:padding="0dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/home"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <EditText
        android:id="@+id/ec_1_campo_1"
        android:layout_width="320dp"
        android:layout_height="49dp"
        android:background="@android:color/background_light"
        android:drawableBottom="@android:color/background_light"
        android:ems="10"
        android:inputType="numberSigned|number|numberDecimal"
        android:textColor="#1B1919"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.148"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.608" />

    <TextView
        android:id="@+id/resultadosView2"
        android:layout_width="208dp"
        android:layout_height="63dp"
        android:background="@drawable/fondo_resultado"
        android:gravity="bottom|right"
        android:text="0"
        android:textColor="#FFFFFF"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.918" />


</androidx.constraintlayout.widget.ConstraintLayout>